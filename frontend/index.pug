// mixins
include html/frame/frame
include html/profile/profile
include html/contact/contact
include html/skills/skills
include html/education/education
include html/experience/experience
include html/software/software

doctype html
html(lang='ru')
    head
        meta(charset='UTF-8')
        title Summary
        meta(name='keywords' content='FSD team, test task, test model')
        meta(name='description' content='Test task for FSD team')
        meta(name='viewport' content='width=device-width' initial-scale='1.0')
    body
        include html/wrapper/wrapper
            include html/wrapper/wrapper__fist-column
                +frame('name here', 'surname', 'graphic designer')
                +profile('profile')
                +contact('contact', 'Main Street, City.', 'contact@domain.com', '555-555-555', 'www.yourweb.com')
                +skills('skills')
            include html/wrapper/wrapper__second-column
                +education('education')
                +experience('experience')
                +software('software')


