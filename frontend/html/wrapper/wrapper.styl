.wrapper
  max-width 1200px
  margin 0 auto

  &__first-column
    width 35%
    float left
    @media screen and (max-width: 850px)
      width auto
      padding-left 10px
      padding-right 10px

  &__second-column
    margin-left 40%
    @media screen and (max-width: 850px)
      margin 0
      padding-left 10px
      padding-right 10px
      float left